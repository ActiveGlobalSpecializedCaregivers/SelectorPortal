$(document).ready(function() {
	
	$("#applicant-email-value").blur(function(){
		var email = $("#applicant-email-value").val();
		
		$("#applicant-email-label").find("span").remove();
		
		var Regex = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		if(!Regex.test(email)){
			$("#applicant-email-label").append(' <span class="applicant_label_error" style="color: #f00 !important;">Enter valid email address; eg:agsc@outlook.com</span>');
			$("#checkEmail").val("Enter valid email address; eg:agsc@outlook.com");
		}else{
			$.get("${pageContext.request.contextPath}/checkCandidate", { email:email } ,function(data){
				if(data.status == 'error') {
					$("#applicant-email-label").append(' <span class="applicant_label_error" style="color: #f00 !important;">'+data.msg+'.</span>');
					$("#checkEmail").val(data.msg);
				}else if(data.status == 'success'){
					$("#applicant-email-label").append(' <span class="applicant_label_error" style="color: green !important;">The mailbox can be used.</span>');
					$("#checkEmail").val("The mailbox can be used.");
				}
			},"json");
		}
	});
	
	
});

	//checkout resume size
	function checkSize(obj){
		var size = obj.files[0].size;
		var maxSize = 5*1024*1024;		//5M
		if(size > maxSize){
			alert("The size of this file can not exceed 5MB!");
			$(obj).val("");
		}		 
	}
</script>
</html>
